{
  "name": "basketball-agent",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-07",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "ai": {
    "binding": "AI"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "BASKETBALL_AGENT",
        "class_name": "BasketballExpertAgent"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["BasketballExpertAgent"]
    }
  ],
  "assets": {
    "directory": "./public/",
    "binding": "ASSETS"
  }
}
